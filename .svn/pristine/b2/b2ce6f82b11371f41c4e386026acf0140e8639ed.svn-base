							<tr role="row">
							<th role="columnheader">法人</th>
							<th role="columnheader">手机号</th>
							<th role="columnheader">营业执照</th>
							<th role="columnheader">公司</th>
							<th role="columnheader">地址</th>
							<th role="columnheader">经销商</th>
							<th role="columnheader">操作</th>
							</tr>
						</thead>
						<tbody role="rowgroup">
							<tr role="row" v-for="(item,id) in list" v-if="id+(page-1)*num>=(page-1)*num && id+(page-1)*num<page*num">
								<td role="cell">{{ item.name }}</td>
								<td role="cell">{{ item.phone }}</td>
								<td role="cell">{{ item.business_license }}</td>
								<td role="cell">{{ item.company }}</td>
								<td role="cell">{{ item.address }}</td>
								<td role="cell">{{ item.dealer_name }}</td>
