									<span @click="delClick(item,id)" class="icon fa-times" style="margin-left:10px;color:#f00;"></span>
									<span title="添加小程序" @click="addProClick(item,id)" class="icon fa-paper-plane" style="margin-left:10px;color:#15d03e;"></span>
									
								</td>
							</tr>
							
